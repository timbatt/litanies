import LitanyRenderer from "../components/LitanyRenderer.astro";
import litanyData from '../json/sacred_heart.json';
const BASE = import.meta.env.BASE_URL;


---

<PostLayout title="The Litany of the Sacred Heart of Jesus" heroImg={`${ BASE }/img/sacred_heart.png` }>
	<LitanyRenderer data={ litanyData } />
</PostLayout>
